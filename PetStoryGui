local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Pet Story Gui v1.1 | Made by red_Jeff", "GrapeTheme")

local Tab = Window:NewTab("Misc")
local MiscSection = Tab:NewSection("Misc")
MiscSection:NewButton("Generate Treats (InGame)", "Continues To Generate Treats until you leave the game.", function()
	while true do

		local event = game:GetService("ReplicatedStorage").Remotes.TreatTouched

		event:FireServer()
		wait(0)
	end
	print("Clicked")
end)

MiscSection:NewButton("AutoHeal", "Heals you automatically when you are low on health.", function()
	while true do

		local v1 = 10
		local event = game:GetService("ReplicatedStorage").Remotes.HealEvent

		event:FireServer(v1)
		wait(0.3)
	end
	print("Clicked")
end)

MiscSection:NewButton("Give 500 million treats (Use in lobby only)", "Gives you 500 million treats and more if you click.", function()
	local v1 = 999999999
	local event = game:GetService("ReplicatedStorage").Remotes.ReturnMoney

	event:FireServer(v1)
	print("Clicked")
end)

local Tab = Window:NewTab("Hats")
local HatsSection = Tab:NewSection("Hats (Yes they save even after you leave.)")
HatsSection:NewButton("Christmas Hat", "Gives you the Christmas Hat", function()
	local v1 = game:GetService("ReplicatedStorage").Hats["Christmas Hat"]
	local event = game:GetService("ReplicatedStorage").Remotes.BoxUnlockEvent

	event:FireServer(v1)
	print("Clicked")
end)

